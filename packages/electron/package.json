{
    "name":  "@webpilot/onboarding",
    "version":  "0.2.1",
    "private":  true,
    "author":  "WebPilot",
    "description":  "WebPilot onboarding UI",
    "main":  "electron/main.js",
    "scripts":  {
                    "dev":  "concurrently \"next dev --port 3000\" \"wait-on http://localhost:3000 \u0026\u0026 electron .\"",
                    "build:next":  "next build",
                    "dist":  "electron-builder --publish never",
                    "dist:win":  "npm run build:next \u0026\u0026 electron-builder --win --publish never",
                    "dist:mac":  "npm run build:next \u0026\u0026 electron-builder --mac --publish never",
                    "dist:linux":  "npm run build:next \u0026\u0026 electron-builder --linux --publish never",
                    "start":  "electron ."
                },
    "dependencies":  {
                         "next":  "^15.0.0",
                         "react":  "^19.0.0",
                         "react-dom":  "^19.0.0"
                     },
    "devDependencies":  {
                            "concurrently":  "^9.0.0",
                            "electron":  "33.4.11",
                            "electron-builder":  "^25.0.0",
                            "wait-on":  "^8.0.0"
                        }
}