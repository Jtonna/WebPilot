{
    "name":  "webpilot-server-for-chrome-extension",
    "version":  "0.1.0",
    "description":  "MCP server for browser control via Chrome extension",
    "main":  "index.js",
    "bin":  "cli.js",
    "scripts":  {
                    "start":  "node index.js",
                    "dev":  "node --watch index.js",
                    "dev:network":  "node --watch index.js --network",
                    "start:network":  "node index.js --network",
                    "build":  "echo Use build:win, build:mac, or build:linux \u0026\u0026 exit 1",
                    "build:win":  "pkg . --target node18-win-x64 --out-path dist",
                    "build:mac":  "pkg . --target node18-macos-x64 --out-path dist",
                    "build:linux":  "pkg . --target node18-linux-x64 --out-path dist"
                },
    "pkg":  {
                "outputPath":  "dist",
                "assets":  [
                               "src/**/*.js",
                               "index.js"
                           ]
            },
    "keywords":  [
                     "mcp",
                     "browser",
                     "chrome",
                     "extension"
                 ],
    "license":  "MIT",
    "dependencies":  {
                         "cors":  "^2.8.5",
                         "express":  "^4.18.2",
                         "uuid":  "^9.0.0",
                         "ws":  "^8.14.2"
                     },
    "devDependencies":  {
                            "@yao-pkg/pkg":  "^6.0.0"
                        }
}